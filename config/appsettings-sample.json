{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Quartz": {
      "SheetsNotificationJob": "0/30 * * * * ?"
    },
    "AzureLog": {
        "WorkspaceId": "AZURE-LOG-WORKSPACE-ID",
        "PrimaryKey": "AZURE-LOG-PRIMARY-KEY"
    },
    "Google": {
        "SpreadsheetId": "GOOGLE-SHEETS-ID",
        "Sheet": "GOOGLE-SHEETS-NAME"
    },
    "Sendgrid": {
        "ApiKey": "SENDGRID-API-KEY",
        "Name": "NAME-OF-SENDER",
        "FromEmail": "FROM-EMAIL"
    },
    "Twilio": {
        "AccountSid": "TWILIO-ACCOUNT-SID",
        "AuthToken": "TWILIO-AUTH-TOKEN",
        "FromNumber": "FROM-PHONE-NUMBER"
    },
    "AllowedHosts": "*"
}